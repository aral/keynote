{
  "name": "vue-keynote",
  "version": "0.6.0",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "vuec src",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rm -rf dist",
    "test": "vue-cli-service test",
    "lint": "echo 'not set'",
    "dev": "vue serve -o example/index.js",
    "example": "./bin/keynote.js serve example/Simple.vue"
  },
  "dependencies": {
    "@keynote/theme-default": "^0.1.0",
    "@vue/cli": "^3.0.0-beta.9",
    "@znck/prop-types": "^0.3.4",
    "de-indent": "^1.0.2",
    "hammerjs": "^2.0.8",
    "lodash.throttle": "^4.1.1",
    "prismjs": "^1.13.0",
    "resize-observer-polyfill": "^1.5.0",
    "tmp": "^0.0.33",
    "velocity-animate": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@vue/cli-plugin-babel": "^3.0.0-alpha.7",
    "@vue/cli-plugin-unit-jest": "^3.0.0-alpha.7",
    "@vue/cli-service": "^3.0.0-alpha.7",
    "@vue/eslint-config-prettier": "^3.0.0-beta.9",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-jest": "^22.0.4",
    "lint-staged": "^7.0.5",
    "node-sass": "^4.7.2",
    "sass-loader": "^7.0.1",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint --",
      "git add"
    ],
    "*.vue": [
      "npm run lint --",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "bin": {
    "keynote": "bin/keynote.js"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "themes/"
  ],
  "style": "dist/vue-keynote.css",
  "main": "dist/vue-keynote.common.js",
  "unpkg": "dist/vue-keynote.umd.js"
}
